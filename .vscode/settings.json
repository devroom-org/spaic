{
  "files.exclude": {
    "**/CMakeFiles": true,
    "**/cmake_install.cmake": true,
    "**/Makefile": true,
    "**/CMakeCache.txt": true,
    "**/*.a": true
  },
  "files.associations": {
    "*.tcc": "cpp",
    "type_traits": "cpp",
    "optional": "cpp",
    "system_error": "cpp",
    "array": "cpp",
    "tuple": "cpp",
    "utility": "cpp",
    "functional": "cpp"
  }
}
